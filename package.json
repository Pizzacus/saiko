	"dependencies": {
		"colors": "^1.1.2",
		"discord.js": "^11.2.1"
	},
	"devDependencies": {
		"babel-cli": "^6.24.1",
		"babel-preset-env": "^1.6.0",
		"eslint": "^4.4.1",
		"jsdoc": "^3.5.4"
	}
